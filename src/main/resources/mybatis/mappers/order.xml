<?xml version="1.0" encoding="UTF-8" ?>
        <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.snowcattle.demo.mapper.OrderMapper">

        <!--<typeAliases>-->
                <!--<typeAlias alias="order" type="com.snowcattle.demo.entity.Order" />-->
        <!--</typeAliases>-->

        <resultMap id = "Order" type="com.snowcattle.demo.entity.Order">
                <result column="order_id" property="orderId" jdbcType="INTEGER"/>
                <result column="user_id" property="userId" jdbcType="INTEGER"/>
                <result column="status" property="status" jdbcType="VARCHAR"/>
        </resultMap>

        <insert id="insertOrder" parameterType="com.snowcattle.demo.entity.Order">
                insert into `t_order` values (#orderId#, #userId#, #status#)
        </insert>

</mapper>